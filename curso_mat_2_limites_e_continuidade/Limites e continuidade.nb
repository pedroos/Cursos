Cell[TextData[{
 "O que \[EAcute] essa p*? ",
 Cell[BoxData[
  FormBox["dq", TraditionalForm]],ExpressionUUID->
  "0e4c5549-7e59-48ac-9ac3-befc14ca2e51"],
 " seria ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"f", "(", 
       RowBox[{"x", "+", "\[CapitalDelta]x"}], ")"}], "-", 
      RowBox[{"f", "(", "x", ")"}]}], "\[CapitalDelta]x"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"x", "+", "\[CapitalDelta]x"}], ")"}], "2"], "+", "2", "-", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["x", "2"], "+", "2"}], ")"}]}], "\[CapitalDelta]x"], 
     "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["x", "2"], "+", 
        RowBox[{"2", "x\[CapitalDelta]x"}], "+", 
        SuperscriptBox["\[CapitalDelta]x", "2"], "+", "2", "-", 
        SuperscriptBox["x", "2"], "-", "2"}], "\[CapitalDelta]x"], "=", 
      RowBox[{
       FractionBox[
        RowBox[{"\[CapitalDelta]x", "(", 
         RowBox[{
          RowBox[{"2", "x"}], "+", "\[CapitalDelta]x"}], ")"}], 
        "\[CapitalDelta]x"], "=", 
       RowBox[{
        RowBox[{"2", "x"}], "+", "\[CapitalDelta]x"}]}]}]}]}], 
   TraditionalForm]],
  FontSize->18,ExpressionUUID->"c269841f-8794-4453-ac70-61c32f41a12c"],
 ", que tende a ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"2", "x"}],
    FontSize->18], TraditionalForm]],ExpressionUUID->
  "bd633b2b-1091-4469-9ac1-22093afb74ce"],
 " conforme ",
 Cell[BoxData[
  FormBox["\[CapitalDelta]x", TraditionalForm]],ExpressionUUID->
  "b17f955e-645a-4ce7-9049-ede3aa5b85c6"],
 " tende a zero."
}], "Text",
