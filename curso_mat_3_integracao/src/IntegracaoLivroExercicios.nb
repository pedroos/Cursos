Cell[TextData[{
 "2. Calcule ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox[
     RowBox[{"\[Integral]", 
      RowBox[{"\[Integral]", "\[Integral]"}]}], "T"], 
    RowBox[{"\[DifferentialD]", "V"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "affb3ebd-744f-4f24-9453-b5b2ba5b357b"],
