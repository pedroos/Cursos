Cell[TextData[{
 "4. Calcule ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox[
     OverscriptBox["\[Integral]", "2"], "0"], 
    RowBox[{
     UnderscriptBox[
      OverscriptBox["\[Integral]", 
       RowBox[{"2", "-", "y"}]], "0"], 
     RowBox[{
      UnderscriptBox[
       OverscriptBox["\[Integral]", 
        RowBox[{"4", "-", 
         SuperscriptBox["y", "2"]}]], "0"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "x"}], 
       RowBox[{"\[DifferentialD]", "z"}], 
       RowBox[{"\[DifferentialD]", "y"}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ccf817e0-5825-4444-ad94-4caaf355dbf9"],
