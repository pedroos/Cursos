  FormBox[
   RowBox[{"R", "=", 
    StyleBox[
     RowBox[{"{", GridBox[{
        {
         RowBox[{"y", "=", 
          RowBox[{
           SuperscriptBox["x", "2"], "+", "1"}]}]},
        {
         RowBox[{"y", "=", 
          RowBox[{"3", "-", "x"}]}]}
       }]}],
     StripOnInput->False,
