Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{"2", "x"}], "-", "3"}], 
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     RowBox[{"3", "x"}], "-", "10"}]], "=", 
   RowBox[{
    RowBox[{
     FractionBox["A", 
      RowBox[{"x", "+", "5"}]], "+", 
     FractionBox["B", 
      RowBox[{"x", "-", "2"}]]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"A", "(", 
         RowBox[{"x", "-", "2"}], ")"}], "+", 
        RowBox[{"B", "(", 
         RowBox[{"x", "+", "5"}], ")"}]}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "+", "5"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"x", "-", "2"}], ")"}]}]], "\[Implies]", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"2", "x"}], "-", "3"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"A", "(", 
         RowBox[{"x", "-", "2"}], ")"}], "+", 
        RowBox[{"B", "(", 
         RowBox[{"x", "+", "5"}], ")"}]}], "\[Implies]", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"2", "x"}], "-", "3"}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"A", " ", "x"}], "-", 
         RowBox[{"2", "A"}], "+", 
         RowBox[{"B", " ", "x"}], "+", 
         RowBox[{"5", "B"}]}], "\[Implies]", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"2", "x"}], "-", "3"}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"x", "(", 
          RowBox[{"A", "+", "B"}], ")"}], "-", 
         RowBox[{"2", "A"}], "+", 
         RowBox[{"5", "B"}]}], "\[Implies]", "\[IndentingNewLine]", 
        RowBox[{
         StyleBox["{",
          StripOnInput->False,
          SpanMaxSize->DirectedInfinity[1]], 
         RowBox[{
          StyleBox[GridBox[{
             {
              StyleBox[
               RowBox[{
                RowBox[{"A", "+", "B"}], "=", "2"}],
               StripOnInput->False,
               SpanMaxSize->DirectedInfinity[1]]},
             {
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"-", "2"}], "A"}], "+", 
                RowBox[{"5", "B"}]}], "=", 
               RowBox[{"-", "3"}]}]}
            }],
           StripOnInput->False,
           SpanMaxSize->DirectedInfinity[1]], 
          StyleBox[".",
           FontSize->14]}]}]}]}]}]}]}]}], TraditionalForm]],
 FontSize->18,ExpressionUUID->"3a4fc7a8-ef65-45dc-84af-e1ffea094b90"]], "Text",\
