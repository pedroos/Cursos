       RowBox[{"\[DifferentialD]", "x"}]}]}]}], "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{
       FractionBox["2", 
        RowBox[{"x", "-", "1"}]], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "+", 
     RowBox[{"\[Integral]", 
      RowBox[{
       FractionBox["3", 
        RowBox[{"x", "+", "3"}]], 
       RowBox[{
        RowBox[{"\[DifferentialD]", "x"}], "."}]}]}]}]}]}], TraditionalForm]],
