Cell["Derivadas de polinomiais (combinar (?)).", "Text",ExpressionUUID->"6609c1b2-222a-4955-86e0-91c12212af2c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"5", 
        SuperscriptBox["x", "2"]}], "+", 
       RowBox[{"3", "x"}], "+", "2"}], ")"}], "'"}], "=", 
    RowBox[{
     RowBox[{"10", "x"}], "+", "3"}]}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"2822978e-2fd9-4f6d-8589-aa25f3228e67"],
 ".\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SqrtBox[
        RowBox[{"3", 
         SuperscriptBox["x", 
          RowBox[{" ", "7"}]]}]], "-", 
       FractionBox[
        SuperscriptBox["x", 
         RowBox[{" ", "5"}]], "5"], "+", "\[Pi]"}], ")"}], "'"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"3", 
         SuperscriptBox["x", 
          RowBox[{"7", "\[CenterDot]", 
           FractionBox["1", "2"]}]]}], ")"}], "'"}], "-", 
      RowBox[{"5", 
       SuperscriptBox["x", "4"]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["7", "2"], "\[CenterDot]", "3"}], 
        SuperscriptBox["x", 
         RowBox[{
          FractionBox["7", "2"], "-", "1"}]]}], "-", 
       RowBox[{"5", 
        SuperscriptBox["x", "4"]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox["7", "2"], "\[CenterDot]", "3"}], 
         SuperscriptBox["x", 
          FractionBox["5", "2"]]}], "-", 
        RowBox[{"5", 
         SuperscriptBox["x", "4"]}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox["7", "2"], "\[CenterDot]", 
          SqrtBox[
           RowBox[{"3", 
            SuperscriptBox["x", "5"]}]]}], "-", 
         RowBox[{"5", 
          SuperscriptBox["x", "4"]}]}], "=", 
        RowBox[{
         RowBox[{"7", 
          SqrtBox[
           RowBox[{"3", 
            SuperscriptBox["x", "5"]}]]}], "-", 
         RowBox[{"10", 
          RowBox[{
           SuperscriptBox["x", "4"], "."}]}]}]}]}]}]}]}], TraditionalForm]],
  FontSize->18,
  FontColor->GrayLevel[0.5],ExpressionUUID->
  "6eadb818-d077-45b0-82d1-77c2f48201c1"]
}], "Text",ExpressionUUID->"fe628c9c-d19a-4a73-b964-208613fe941f"],

