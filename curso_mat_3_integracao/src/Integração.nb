 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{"3", " ", "sen", " ", "u", " ", 
      RowBox[{"\[DifferentialD]", "u"}]}]}], "=", 
    RowBox[{"3", 
     RowBox[{"\[Integral]", 
      RowBox[{"sen", " ", "u", 
       RowBox[{"\[DifferentialD]", "u"}]}]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"f70e1ac9-5564-40f3-916e-7a019865df95"],
 ", que \[EAcute] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"3", " ", "-", 
    RowBox[{"cos", " ", "u"}], "+", "C"}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"43ba7e0f-8702-4058-bc6d-ca00c366f2c5"],
 ". Agora substituindo ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]],
  FontSize->24,ExpressionUUID->"7e312575-8ff1-440e-9ef4-b510d125faa6"],
 " de volta, \[EAcute] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", "3"}], " ", "cos", " ", 
     FractionBox["x", "3"]}], "+", "C"}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"8c46c555-9732-4015-a56b-62baf82c7b8e"],
 "."
}], "Text",ExpressionUUID->"68e50ef9-b895-4084-9331-f86e645ca200"],

