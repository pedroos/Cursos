     RowBox[{
      RowBox[{
       RowBox[{"-", "y"}], "\[Implies]", 
       RowBox[{"-", "x"}]}], "=", 
      RowBox[{
       RowBox[{"y", "\[Implies]", "y"}], "=", 
       RowBox[{
        RowBox[{"-", "x"}], "=", 
        RowBox[{"f", "(", "x", ")"}]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
