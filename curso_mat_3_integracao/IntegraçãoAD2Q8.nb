Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{"A", "=", 
   RowBox[{
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Integral]", "0"], "\[Theta]"], 
     RowBox[{
      OverscriptBox[
       UnderscriptBox["\[Integral]", "0"], "r"], 
      RowBox[{"r", " ", 
       RowBox[{"\[DifferentialD]", "r"}], 
       RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}]}], "=", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      OverscriptBox[
       UnderscriptBox["\[Integral]", "0"], 
       RowBox[{"2", "\[Pi]"}]], 
      RowBox[{
       OverscriptBox[
        UnderscriptBox["\[Integral]", "0"], 
        RowBox[{"2", "-", 
         RowBox[{"2", "sen\[Theta]"}]}]], 
       RowBox[{"r", 
        RowBox[{"\[DifferentialD]", "r"}], 
        RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}]}], "=", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        OverscriptBox[
         UnderscriptBox["\[Integral]", "0"], 
         RowBox[{"2", "\[Pi]"}]], 
        FractionBox[
         SuperscriptBox["r", "2"], "2"]}], 
       OverscriptBox[
        UnderscriptBox["|", 
         RowBox[{"r", "=", "0"}]], 
        RowBox[{"r", "=", 
         RowBox[{"2", "-", 
          RowBox[{"2", "sen\[Theta]"}]}]}]], 
       RowBox[{"\[DifferentialD]", "\[Theta]"}]}], "=", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        UnderscriptBox[
         OverscriptBox["\[Integral]", 
          RowBox[{"2", "\[Pi]"}]], "0"], 
        RowBox[{
         FractionBox[
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"2", "-", 
             RowBox[{"2", "sen\[Theta]"}]}], ")"}], "2"], "2"], 
         RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}], "=", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         UnderscriptBox[
          OverscriptBox["\[Integral]", 
           RowBox[{"2", "\[Pi]"}]], "0"], 
         RowBox[{
          FractionBox[
           RowBox[{"4", "-", 
            RowBox[{"2", "\[CenterDot]", "2", "\[CenterDot]", 
             RowBox[{"(", 
              RowBox[{"2", "sen\[Theta]"}], ")"}]}], "+", 
            RowBox[{"4", 
             SuperscriptBox["sen", "2"], "\[Theta]"}]}], "2"], 
          RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}], "=", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{
           UnderscriptBox[
            OverscriptBox["\[Integral]", 
             RowBox[{"2", "\[Pi]"}]], "0"], "2"}], "-", 
          RowBox[{"4", "sen\[Theta]"}], "+", 
          RowBox[{"2", 
           SuperscriptBox["sen", "2"], "\[Theta]", 
           RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}], "=", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"2", "\[Theta]"}], "+", 
            RowBox[{"4", "cos\[Theta]"}], "+", 
            RowBox[{"2", 
             RowBox[{"(", 
              RowBox[{
               FractionBox["\[Theta]", "2"], "-", 
               FractionBox[
                RowBox[{"sen", " ", "2", "\[Theta]"}], "4"]}], ")"}]}]}], 
           UnderscriptBox[
            OverscriptBox["|", 
             RowBox[{"\[Theta]", "=", 
              RowBox[{"2", "\[Pi]"}]}]], 
            RowBox[{"\[Theta]", "=", "0"}]]}], "=", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"2", "\[Theta]"}], "+", 
             RowBox[{"4", "cos\[Theta]"}], "+", "\[Theta]", "-", 
             FractionBox[
              RowBox[{"sen", " ", "2", "\[Theta]"}], "2"]}], 
            UnderscriptBox[
             OverscriptBox["|", 
              RowBox[{"\[Theta]", "=", 
               RowBox[{"2", "\[Pi]"}]}]], 
             RowBox[{"\[Theta]", "=", "0"}]]}], "=", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"4", "\[Pi]"}], "+", "4", "+", 
             RowBox[{"2", "\[Pi]"}], "-", "4"}], "=", 
            RowBox[{"6", 
             RowBox[{"\[Pi]", 
              StyleBox[".",
               FontSize->14]}]}]}]}]}]}]}]}]}]}]}]}], TraditionalForm]],
 FontSize->24,ExpressionUUID->"c91920fb-9343-423a-8c7e-2addcfd73bee"]], "Text",\
