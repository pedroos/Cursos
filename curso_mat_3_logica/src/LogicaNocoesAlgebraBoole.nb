     RowBox[{"\[ForAll]", 
      RowBox[{"x", ":", 
       RowBox[{"\[Not]", 
        RowBox[{"x", "."}]}]}]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"718b1690-d952-40b9-9963-138ac6b7fb67"]
}], "Text",ExpressionUUID->"ee737785-3247-4869-9975-cc650f0508c5"],

